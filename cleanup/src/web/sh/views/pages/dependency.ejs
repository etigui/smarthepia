<script>

    onLoad();

    function onLoad(){
        genDepenedencyTable();
    }

    function loadBadge(){
        $.ajax({
            url: "/alarm/badge",
            type: "get",
            dataType: "json",
            success: function (response) {
                var span = $('#alarmBadge');
                updateBadge(span, response.atype, response.count);
            }
        });
    }

    function genDepenedencyTable(){
        $('#dependencyTable').dataTable({
            dom: 'Bfrtip',
            buttons: [{
                extend: 'collection',
                text: 'Option',
                className: 'btn btn-primary',
                buttons: [
                    {
                        extend: 'pdf',
                        text: "Export as PDF",
                        exportOptions: {

                            columns: [0, 1, 2, 3, 4, 5 ]

                        }
                    },
                    {
                        extend: 'csv',
                        text: "Export as CSV",
                        exportOptions: {
                            columns: [ 0, 1, 2, 3, 4, 5 ]

                        }
                    },{
                        extend: 'excel',
                        text: "Export as Excel",
                        exportOptions: {
                            columns: [ 0, 1, 2, 3, 4, 5 ]

                        }
                    },{
                        extend: 'copy',
                        text: "Copy to clipboard",
                        exportOptions: {
                            columns: [ 0, 1, 2, 3, 4, 5 ]

                        }
                    }
                    ,'pageLength','colvis'
                ]}],
            lengthMenu: [[10, 25, 50, -1], [10, 25, 50, "All"]],
            autoWidth: false,
            destroy: true,
            searching: true,
            type: 'GET',
            ajax: "/dependency/listall",
            columns: [
                { data: "depName"},
                { data: "divName"},
                { data: "ip"},
                { data: "port"},
                { data: "method"},
                { data: "comment"},
                {},
                {data: "ddId"},
                {data: "dId"}
            ], columnDefs: [{
                targets: 6,
                searching: false,
                orderable: false,
                data: null,
                className: "text-center align-middle last",
                render: function (data, type, full, meta){
                    return '<div class="btn-group" >\n' +
                        '    <button type="button" class="btn btn-primary dropdown-toggle"  data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">\n' +
                        '        Action\n' +
                        '    </button>\n' +
                        '    <div class="dropdown-menu" x-placement="bottom-start">\n' +
                        '        <button id="editDependency" class="dropdown-item" type="button" data-toggle="modal" data-target="#modalEditDependency" >Edit</button>\n' +
                        '        <button id="deleteDependencySubmit" class="dropdown-item" type="button">Delete</button>\n' +
                        '    </div>\n' +
                        '</div>';
                }
            }, {targets: 0, className: "text-center align-middle"},
                {targets: 1, className: "text-center align-middle"},
                {targets: 2, className: "text-center align-middle"},
                {targets: 3, className: "text-center align-middle"},
                {targets: 4, className: "text-center align-middle"},
                {targets: 5, className: "text-center align-middle"},
                {targets: 7,
                    "visible": false,
                    "searchable": false},
                {targets: 8,
                    "visible": false,
                    "searchable": false}]
        });
    }

    // Sens ajax request to add, delete, edit dependency
    function setDependency(url, datas){
        $.ajax({
            url: url,
            type: "post",
            dataType: "json",
            data: datas,
            success: function (response) {
                if (response.status === "success") {
                    $('#dependencyTable').DataTable().ajax.reload();
                    $("#createDependencySubmit").get(0).reset();
                    toastr.success(response.message, response.status, {closeButton: true});
                } else if (response.status === "error") {
                    toastr.error(response.message, response.status, {closeButton: true});
                } else {
                    toastr.error("Undefined", "error", {closeButton: true});
                }
            }
        });
    }

    // Edit dependency
    $("#dependencyTable tbody").on("click","#editDependency", function () {
        var nRow = $(this).parents('tr')[0];
        var oTable = $('#dependencyTable').dataTable();
        var aData = oTable.fnGetData(nRow);
        $('#editDdId').val(aData.ddId);
        $('#editDId').val(aData.dId);
        $('#editDeviceName').val(aData.divName);
        $('#editIp').val(aData.ip);
        $('#editPort').val(aData.port);
        $('#editMethod').val(aData.method);
        $('#editComment').val(aData.comment);
    });

    // Submit data to delete dependency
    $("#dependencyTable tbody").on("click","#deleteDependencySubmit", function () {
        event.preventDefault();
        var nRow = $(this).parents('tr')[0];
        var oTable = $('#dependencyTable').dataTable();
        var aData = oTable.fnGetData(nRow);
        setDependency("/dependency/delete", {dDid: aData.ddId, dId: aData.dId, name: aData.depName});
    });


    // Submit edit dependency
    $( "#editDependencySubmit" ).submit(function( event ) {
        event.preventDefault();
        console.log($("#editIp").val());
        if(validateIP46($("#editIp").val())){
            var data = $('#editDependencySubmit').serialize();
            setDependency("/dependency/edit", data);
        }else{
            toastr.error("IP must be in IPv4 or IPv6 format", "error", {closeButton: true});
        }
        $("#modalEditDependency").modal('toggle');
    });

    // Check ip
    function validateIP46(str){

        // IPV6 and IPV4 with hostname
        //return /((^\s*((([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])\.){3}([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5]))\s*$)|(^\s*((([0-9A-Fa-f]{1,4}:){7}([0-9A-Fa-f]{1,4}|:))|(([0-9A-Fa-f]{1,4}:){6}(:[0-9A-Fa-f]{1,4}|((25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)(\.(25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)){3})|:))|(([0-9A-Fa-f]{1,4}:){5}(((:[0-9A-Fa-f]{1,4}){1,2})|:((25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)(\.(25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)){3})|:))|(([0-9A-Fa-f]{1,4}:){4}(((:[0-9A-Fa-f]{1,4}){1,3})|((:[0-9A-Fa-f]{1,4})?:((25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)(\.(25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)){3}))|:))|(([0-9A-Fa-f]{1,4}:){3}(((:[0-9A-Fa-f]{1,4}){1,4})|((:[0-9A-Fa-f]{1,4}){0,2}:((25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)(\.(25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)){3}))|:))|(([0-9A-Fa-f]{1,4}:){2}(((:[0-9A-Fa-f]{1,4}){1,5})|((:[0-9A-Fa-f]{1,4}){0,3}:((25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)(\.(25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)){3}))|:))|(([0-9A-Fa-f]{1,4}:){1}(((:[0-9A-Fa-f]{1,4}){1,6})|((:[0-9A-Fa-f]{1,4}){0,4}:((25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)(\.(25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)){3}))|:))|(:(((:[0-9A-Fa-f]{1,4}){1,7})|((:[0-9A-Fa-f]{1,4}){0,5}:((25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)(\.(25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)){3}))|:)))(%.+)?\s*$))|(^\s*((?=.{1,255}$)(?=.*[A-Za-z].*)[0-9A-Za-z](?:(?:[0-9A-Za-z]|\b-){0,61}[0-9A-Za-z])?(?:\.[0-9A-Za-z](?:(?:[0-9A-Za-z]|\b-){0,61}[0-9A-Za-z])?)*)\s*$)/.test(str);
        // IPV6 and IPV4 without hostname
