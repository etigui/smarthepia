                                <fieldset class="form-group floating-label-form-group">
                                    <label for="">Device name
                                        <span class="danger">*</span>
                                    </label>
                                    <input id="editDeviceName" name="editDeviceName" type="text" class="form-control square" placeholder="Device name" required autocomplete="off">
                                </fieldset>
                                <fieldset class="form-group floating-label-form-group">
                                    <label for="">IP address
                                        <span class="danger">*</span>
                                    </label>
                                    <input id="editIp" name="editIp" type="text" class="form-control square" placeholder="IP address" required autocomplete="off">
                                </fieldset>
                                <fieldset class="form-group floating-label-form-group">
                                    <label for="">Port
                                        <span class="danger">*</span>
                                    </label>
                                    <input id="editPort" name="editPort" type="text" class="form-control square" placeholder="Port" required autocomplete="off">
                                </fieldset>
                                <fieldset class="form-group floating-label-form-group">
                                    <label for="title">Method</label>
                                    <select name="editMethod" id="editMethod" class="form-control square" disabled>
                                        <option value="" selected="" disabled="" hidden="">Select method</option>
                                        <option value="Ping">Ping</option>
                                        <option value="REST/HTTP">REST/HTTP</option>
                                    </select>
                                </fieldset>
                                <fieldset class="form-group floating-label-form-group">
                                    <label for="">Comment</label>
                                    <input id="editComment" name="editComment" type="text" class="form-control square" placeholder="Comment" autocomplete="off">
                                </fieldset>
                                <input id="editDdId" name="editDdId" type="text" hidden>
                                <input id="editDId" name="editDId" type="text" hidden>
                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-warning mr-1" data-dismiss="modal"><i class="ft-x"></i> Cancel</button>
                                <button type="submit" class="btn btn-primary"><i class="fa fa-check-square-o"></i> Save</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
