    // Create new dependency device
    $("#createDependencySubmit").submit(function( event ) {
        event.preventDefault();

        // Check if all the IP are valid
        var validIp = true;
        $('.depdIp').each(function(i, obj) {
            if(!validateIP46($(this).val())){
                validIp = false;
            }
        });

        // If on or more IP are in error
        if (!validIp) {
            toastr.error("IP must be in IPv4 or IPv6 format", "error", {closeButton: true});
        } else {

            // Select error if not selected
            if($("#depdMethod option:selected").text() === "Select method") {
                toastr.error("You must choose a method", "error", {closeButton: true});
            }else {

                $.ajax({
                    url: "/dependency/create",
                    type: "post",
                    dataType: "json",
                    data: $('#createDependencySubmit').serialize(),
                    success: function (response) {

                        // TODO reload dependency inventory
                        $('#dependencyTable').DataTable().ajax.reload();
                        if (response.status === "success") {
                            $("#createDependencySubmit").get(0).reset();
                            toastr.success(response.message, response.status, {closeButton: true})
                        } else if (response.status === "error") {
                            toastr.error(response.message, response.status, {closeButton: true})
                        } else {
                            toastr.error("Undefined", "error", {closeButton: true})
                        }
                    }
                });
            }
        }
    });

    // BEGIN Server time section /////////////////////////////////////////////////////////////
    // Local and servers time
    var timeLocal = new Date();
    var timeServer = new Date("<%= dateTime %>");

    //C alculate the difference (returns milliseconds)
    var millDiff = timeLocal - timeServer;

    // Init the clock on loading of page
    window.onload = function () {
        //set the interval so clock ticks
        var timeClock = setInterval("timeTick()", 10);
    };
    // END Server time section /////////////////////////////////////////////////////////////

    <% if((typeof permission !== 'undefined') && permission > 0) {%>
